<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manthan.capsulecalendar">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".camera" />
        <activity android:name=".homeScreen" />
        <activity android:name=".OcrGraphic" />
        <activity android:name=".OcrDetectorProcessor" />
        <activity android:name=".ui.CameraSource" />
        <activity android:name=".ui.CameraSourcePreview" />
        <activity android:name=".ui.GraphicOverlay" />
        <activity android:name=".OcrCapture" />
        <activity android:name=".OcrCaptureActivity" />
        <activity android:name=".confirmation"></activity>
    </application>

</manifest>